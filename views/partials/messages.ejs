<div id="messages">
  <% Object.keys(messages).forEach(function (type) { %> <%
  messages[type].forEach(function (message) { %>
  <div class="alert alert-dismissible mb-3 alert-<%= type %>">
    <%= message %>
    <button class="close" data-dismiss="alert" type="button">&times;</button>
  </div>
  <% }) %> <% }) %>
</div>
